{"ast":null,"code":"import EventBus from './eventBus';\nexport default {\n  el: '#registerModal',\n  data() {\n    return {\n      regUsername: \"\",\n      regPassword: \"\",\n      lastRegPassword: \"\"\n    };\n  },\n  methods: {\n    showRegisterModal() {\n      this.$el.style.display = \"block\";\n    },\n    hideRegisterModal() {\n      this.$el.style.display = \"none\";\n    },\n    showLoginAndHideRegister() {\n      this.hideRegisterModal();\n      loginModal.showLoginModal();\n    },\n    register() {\n      if (!this.regUsername) {\n        alert(\"用户名不能为空\");\n      } else if (!this.regPassword) {\n        alert(\"密码不能为空\");\n      } else if (!this.lastRegPassword) {\n        alert(\"请再次输入密码\");\n      } else if (this.regPassword !== this.lastRegPassword) {\n        alert(\"两次输入的密码不一致\");\n      } else {\n        axios({\n          method: \"post\",\n          url: 'http://localhost:3000/users',\n          data: {\n            username: this.regUsername,\n            password: this.regPassword\n          },\n          headers: {\n            \"Content-Type\": \"application/json\"\n          }\n        }).then(function (res) {\n          // console.log(res.data);\n          // 处理注册成功的逻辑，例如隐藏注册模态框\n          alert('注册成功');\n          this.hideRegisterModal();\n        }.bind(this)).catch(function (error) {\n          console.log(error);\n          // 可以在此处显示错误信息\n          alert('注册失败：' + error.message);\n        });\n      }\n    }\n  },\n  mounted() {\n    document.getElementById(\"registerBtn\").addEventListener(\"click\", this.showRegisterModal);\n    document.getElementById(\"return\").addEventListener(\"click\", this.showLoginAndHideRegister);\n    document.getElementsByClassName(\"close\")[1].addEventListener(\"click\", this.hideRegisterModal);\n    EventBus.$on('register-modal-show', () => {\n      this.showRegisterModal();\n    });\n  }\n};","map":{"version":3,"names":["EventBus","el","data","regUsername","regPassword","lastRegPassword","methods",